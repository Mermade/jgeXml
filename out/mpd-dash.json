    "SegmentBaseType": {
      "type": "object",
      "properties": {
        "Initialization": {
          "$ref": "#/definitions/URLType"
        },
        "RepresentationIndex": {
          "$ref": "#/definitions/URLType"
        },
        "timescale": {
          "type": "integer"
        },
        "presentationTimeOffset": {
          "type": "integer"
        },
        "indexRange": {
          "type": "string"
        },
