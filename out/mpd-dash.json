      "type": "object",
      "properties": {
        "serviceLocation": {
          "type": "string"
        },
        "byteRange": {
          "type": "string"
        },
        "availabilityTimeOffset": {
          "type": "number"
        },
        "availabilityTimeComplete": {
          "type": "boolean"
        }
      },
      "additionalProperties": false
    },
    "ProgramInformationType": {
      "type": "object",
      "properties": {
        "Title": {
          "type": "string"
        },
        "Source": {
          "type": "string"
        },
        "Copyright": {
          "type": "string"
        },
        "lang": {
          "pattern": "[a-zA-Z]{1,8}(-[a-zA-Z0-9]{1,8})*",
          "type": "string"
        },
        "moreInformationURL": {
          "format": "uri",
          "type": "string"
        }
      },
      "additionalProperties": false
    },
    "DescriptorType": {
      "type": "object",
      "properties": {
