    "MetricsType": {
      "type": "object",
      "properties": {
        "Reporting": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/DescriptorType"
          }
        },
        "Range": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/RangeType"
          }
        },
        "metrics": {
          "type": "string"
        }
      },
      "required": [
        "Reporting",
        "metrics"
      ],
      "additionalProperties": false
    },
    "RangeType": {
      "type": "object",
      "properties": {
        "starttime": {
          "pattern": "^(-)?P(?:([0-9,.]*)Y)?(?:([0-9,.]*)M)?(?:([0-9,.]*)W)?(?:([0-9,.]*)D)?(?:T(?:([0-9,.]*)H)?(?:([0-9,.]*)M)?(?:([0-9,.]*)S)?)?$",
          "type": "string"
        },
        "duration": {
          "pattern": "^(-)?P(?:([0-9,.]*)Y)?(?:([0-9,.]*)M)?(?:([0-9,.]*)W)?(?:([0-9,.]*)D)?(?:T(?:([0-9,.]*)H)?(?:([0-9,.]*)M)?(?:([0-9,.]*)S)?)?$",
          "type": "string"
        }
      },
      "additionalProperties": false
    },
    "PresentationType": {
      "enum": [
        "static",
        "dynamic"
      ]
    },
    "RatioType": {
      "type": "string",
      "pattern": "[0-9]*:[0-9]*"
    },
    "FrameRateType": {
      "type": "string",
      "pattern": "[0-9]*[0-9](/[0-9]*[0-9])?"
    },
    "ConditionalUintType": {
      "type": "object"
    },
    "StringNoWhitespaceType": {
      "type": "string",
      "pattern": "[^\\r\\n\\t \\p{Z}]*"
    },
    "SAPType": {
      "type": "integer"
    },
    "VideoScanType": {
      "enum": [
        "progressive",
        "interlaced",
        "unknown"
      ]
    },
    "StringVectorType": {
      "type": "object"
    },
    "UIntVectorType": {
      "type": "object"
    }
