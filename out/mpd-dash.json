        "SegmentBase": {
          "$ref": "#/definitions/SegmentBaseType"
        },
        "SegmentList": {
          "$ref": "#/definitions/SegmentListType"
        },
        "SegmentTemplate": {
          "$ref": "#/definitions/SegmentTemplateType"
