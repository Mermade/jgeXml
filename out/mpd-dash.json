        "frameRate": {
          "$ref": "#/definitions/FrameRateType"
        },
        "audioSamplingRate": {
          "type": "string"
        },
        "mimeType": {
          "type": "string"
        },
        "segmentProfiles": {
          "type": "string"
        },
        "codecs": {
          "type": "string"
        },
        "maximumSAPPeriod": {
          "type": "number"
