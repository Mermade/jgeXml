      "type": "object",
      "properties": {
        "S": {
          "type": "array",
          "items": {
            "type": "object",
            "properties": {
              "t": {
                "type": "integer"
              },
              "d": {
                "type": "integer"
              },
              "r": {
                "type": "integer"
              }
            },
            "additionalProperties": false,
            "required": [
              "d"
            ]
          }
        }
      },
      "required": [
        "S"
      ],
      "additionalProperties": false
